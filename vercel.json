{
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "buildCommand": "prisma generate && next build",
  "installCommand": "yarn install",
  "github": {
    "autoAlias": false
  },
  "crons": [
    {
      "path": "/api/cron/tracking-notifications",
      "schedule": "*/15 * * * *"
    }
  ]
}
